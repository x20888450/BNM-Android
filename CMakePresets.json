{
	"version": 6,
	"cmakeMinimumRequired": {
		"major": 3,
		"minor": 23,
		"patch": 0
	},
	"configurePresets": [
		{
			"name": "release",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Release",
				"CMAKE_CXX_FLAGS_RELEASE": "-w -Wl,--as-needed -Wl,--exclude-libs,ALL -Wl,--gc-sections -Wl,--strip-all -Oz -flto -faddrsig -ffunction-sections -fdata-sections -fvisibility=hidden -fstack-protector"
			}
		},
		{
			"name": "action",
			"inherits": "release",
			"cacheVariables": {
				"CMAKE_SYSTEM_NAME": "Android",
				"CMAKE_ANDROID_ARCH": "arm",
				"CMAKE_ANDROID_ARCH_ABI": "armeabi-v7a",
				"CMAKE_ANDROID_ARM_MODE": "ON",
				"CMAKE_ANDROID_ARM_NEON": "ON",
				"CMAKE_CXX_COMPILER": "clang++",
				"CMAKE_CXX_STANDARD": "20",
				"CMAKE_SYSTEM_VERSION": "27"
			}
		}
	]
}